{
  "compilerOptions": {
    "target": "ES6",
    "useDefineForClassFields": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl":".",
  },
  "include": ["src/**/*.ts"],
  "exclude":["node_modules"]
}
